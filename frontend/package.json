{
  "name": "office-check-in",
  "version": "3.6.3",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-process-helper": "ng build process-helper --prod",
    "build-office-check-in-dev": "ng build office-check-in",
    "build-office-check-in": "ng build office-check-in --prod",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare-deploy-dev": "npm run build-process-helper && npm run lint && npm run test && npm run build-office-check-in-dev",
    "prepare-deploy": "npm run build-process-helper && npm run lint && npm run test && npm run build-office-check-in",
    "deploy-dev": "firebase use dev && npm run prepare-deploy-dev && firebase deploy --only hosting:dev",
    "deploy-prod": "firebase use prod && npm run prepare-deploy && firebase deploy --only hosting:prod"
  },
  "dependencies": {
    "@angular/animations": "~9.1.11",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/fire": "6.0.1",
    "@angular/forms": "~9.1.11",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "@ngx-pwa/local-storage": "^9.0.3",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^5.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.6.5",
    "cross-fetch": "^3.0.5",
    "firebase": "^7.24.0",
    "mat-progress-buttons": "^9.1.1",
    "moment": "^2.27.0",
    "proxy-polyfill": "^0.3.2",
    "raven-js": "^3.27.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.900",
    "@angular-devkit/build-angular": "~0.901.8",
    "@angular-devkit/build-ng-packagr": "~0.901.12",
    "@angular/cli": "~9.1.8",
    "@angular/compiler-cli": "~9.1.11",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.47",
    "codelyzer": "^5.1.2",
    "firebase-tools": "^8.16.2",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-spec-reporter": "0.0.32",
    "ng-packagr": "^9.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
